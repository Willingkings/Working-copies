body {
    background: url("../images/bodyBg.png") no-repeat;
    background-size: cover;
    color: #fff;
    padding: 0.2rem;
}
.red{
    color:#D53D4A;
}
.green{
    color:#15BD50;
}
.yellow{
    color:#F9CD50;
}

.content {

}
.item-left {
    float: left;
    width: 25%;
}
.cur-time {
    text-align: center;
    font-size: 0.3rem;
    padding: 0.3rem 0 0.3rem 0.1rem;
}
.item-left_title {
    font-size: 0.3rem;
    text-align: center;
    color: #0eeeed;
}
.item-left_benefits, .item-left_income, .item-left_user, .item-right_traffic,
.item-right_parking, .item-right_present, .item-middle_map, .item-middle_tickets {
    display: inline-block;
    width:100%;
    position:relative;
}
.item-left_bg, .item-right_bg{
    position:absolute;
    left:0;
    right:0;
    top:0;
    bottom: 0;
    z-index: 9;
}
.item-middle_bg {
    position:absolute;
    left: 0.15rem;
    right: 0.15rem;
    top:0;
    bottom: 0;
    z-index: 9;
}
.item-middle {
    float: left;
    width: 50%;
}
.item-middle_title {
    display: inline-block;
    width:80%;
    margin-left: 10%;
    position:relative;
}
.map {
    float: left;
    width: 60%;
    height: 100%;
}
.accounted {
    float: right;
    width: 40%;
    height: 100%;
    padding: 4% 4% 1% 0;
}
.accounted>div {
    height: 33%;
    position: relative;
}
.accounted_title {
    display: block;
    font-size: 0.12rem;
    color: #0eefeb;
    background: #1c3247;
    padding: 0.05rem;
}
.diagram {
    height: 100%;
}
.combined {
    position: absolute;
    bottom: 0.1rem;
    right: 0.35rem;
}
.combined span {
    color: #ffd500;
    font-size: 0.20rem;
    font-style: normal;
}
.combined span i {
    color: #ffd500;
    font-size: 70%;
    font-style: normal;
}




.item_content{
    position:absolute;
    left:0;
    right:0;
    top:0;
    bottom: 0;
    z-index: 99
}
.item-right {
    float: left;
    width: 25%;
}
.item-right .date{
    text-align: center;
    font-size: 0.2rem;
    padding: 0.6rem 0 0.75rem;
}

