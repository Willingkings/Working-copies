/**                                          H                                                 **/
/**             数字模块                **/

.numBorder {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    padding: 0.2rem;
    font-size: 0.25rem;
    z-index: 100;
}
.numBorder > div {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.numBorder > div > div {

}
.numBorder > div > div > p {
    text-align: center;
    font-size: 0.2rem;
    color: #85A3C8;
}
.numBorder > div > div > .parent {
    display: flex;
    padding: 0;

    overflow: hidden;
}
.numBorder .parent > li {
    position: relative;
    width: .27rem;
    height: 0.35rem;
    text-align: center;
    margin-right: 0.03rem;
    background-position: center;
    background-repeat: no-repeat;
}
.numBorderTop .parent > li {
    background-image: url("../images/numRed.png");
}
.numBorderBorrom .parent > li {
    background-image: url("../images/numBlue.png");
}
.numBorderTop .numBorderMoney {
    position: relative;
}
.numBorderTop .numBorderMoney > ul::after {
    position: absolute;
    top: -0.02rem;
    left: -0.25rem;
    display: block;
    width: .27rem;
    height: .4rem;
    background-image: url(../images/￥@2x.png);
    background-position: center;
    background-repeat: no-repeat;
    content: "";
}
.numBorderTop .numBorderMoney > li:nth-last-of-type(5) {
    position: relative;
}
.numBorder .parent > li > ul {
    position: absolute;
    top: 0;
    left: 0;
    width: .25rem;
    padding: 0;
}
.numBorder .parent > li > ul > li {
    width: .27rem;
    height: 0.35rem;
    line-height: 0.35rem;
}
.donghua {
    transform: translateY(-1rem);
    transition: all 2s linear;
}
/**                                          H                                                 **/
/**             柱形模板                **/
.cylindricalityBoy {
    position: relative;
    width: 100%;
    height: 100%;
    padding: .1rem .2rem;
}
.cylindricalityBoy > p {
    color: #27E4DE;
    font-size: .2rem;
}
.cylindricalityBoy > p {
    font-weight: bold;
    font-size: .2rem;
    color: #27E4DE;
}
.cylindricalityBoyTitele {
    display: flex;
    padding-left: .3rem;
    position: absolute;
    z-index: 100;
    top: 15%;
}
.cylindricalityBoyTitele > div {
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    width: .6rem;
    height: .6rem;
    margin-right: .2rem;
}
.cylindricalityBoyTitele > div > img {
    position: absolute;
    height: 100%;
}
.cylindricalityBoyTitele > div > p {
    position: absolute;
    margin: 0;
    font-size: 0.1rem;
}
.cylindricalityBoyTitele > div > span {
    position: absolute;
    display: block;
    font-size: 0.1rem;
    bottom: -0.2rem;
    white-space: nowrap;
}
.Titele1 .cylindricalityBoyTitele > div > span {
    color: #1F77A0;
}
.Titele2 .cylindricalityBoyTitele > div > span {
    color: #FB4B60;
}
.Titele3 .cylindricalityBoyTitele > div > span {
    color: #2EB596;
}
.Titele4 .cylindricalityBoyTitele > div > span {
    color: #fff;
}
#container,#container1,#container2 {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 99;
}
/**                                          H                                                 **/
/**             景区                **/
.scenicAreaBox {
    display: flex;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    padding: .2rem 0;
}
.scenicAreaBox > div {
    position: relative;
    width: 50%;
    height: 100%;
}
.scenicAreaBox > div > p {
    padding-left: 1rem;
    font-size: .3rem;
    color: #27E4DE;
}
.scenicAreaBox > div > div {
    position: absolute;
    display: flex;
    margin-left: 18%;
    margin-top: 9%;
    z-index: 100;
}
.scenicAreaBox > div > div.differentiate > div {
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    width: .6rem;
    height: .6rem;
    margin-right: .2rem;
}
.scenicAreaBox  div > img {
    position: absolute;
    left:0;
    right:0;
    width: 100%;;
    height: 100%;

}
.scenicAreaBox > div > div  div > p {
    position: absolute;
    margin: 0;
    font-size: 0.1rem;

}
.scenicAreaBox .differentiate div > span {
    position: absolute;
    display: block;
    font-size: 0.1rem;
    bottom: -0.3rem;
    white-space: nowrap;

}
.scenicAreaBox  div > span span {
    display: inline-block;
}
#container3,#container4 {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    margin: 0;
    z-index: 99;
}
/**                                          变身                                                **/

#transfiguration{
    position: absolute;
    left: 0;
    top: -0.3rem;
    right: 0;
    margin: 0 auto;
    /* right: 0; */
    width: 2.6rem;
    height: 2.2rem;
    z-index: 500;
}
#transfiguration img{
    display: block;
    width: 100%;
    height: 100%;
}

